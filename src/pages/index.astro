---
import { getCollection } from "astro:content";
import BaseLayout from "../layouts/base-layout.astro";

const workItems = await getCollection("work");
---

<BaseLayout>
  {
    workItems.map((item: any) => (
      <a href={"/work/" + item.data.slug}>{item.data.title}</a>
    ))
  }
</BaseLayout>
